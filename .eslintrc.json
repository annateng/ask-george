{
    "env": {
        "node": true,
        "commonjs": true,
        "es2020": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11
    },
    "rules": {
      "no-console": ["error", { "allow": ["warn", "error"]}],
      "no-use-before-define": ["error", "nofunc"]
    }
}
